            id="mantisChat"
            companyId="3403"
            probeId="60ff7b7942e6824c77cf5c70"
            chatPageUrl="https://demo2.bjmantis.net/chat/t1/chat.html?mcid=3403&mpid=60ff7b7942e6824c77cf5c70"
    />
</view>
