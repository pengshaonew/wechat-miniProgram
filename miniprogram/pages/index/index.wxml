<view class="testsite">
    <view class="testsite_form">
        <view class="ctitle">经济师报名条件自助查询</view>
        <form catchsubmit="handleSubmit" >
            <view class="section">
                <picker bindchange="bindPickerChange" value="{{objectArray.id}}" range="{{array}}" name="content">
                    {{index==''?"请选择您的学历":array[index]}}
                </picker>
            </view>
            <view class="section">
                <input class="leaveInput" type="text" name="username" placeholder="请输入您的姓名" />
            </view>
            <view class="section2">
                <input class="leaveInput" name="phone" placeholder="请输入您的手机号"/>
            </view>
            <view class="tips">（请认真填写，稍后将以短信或语音方式通知您）</view>
            <button type="primary" formType="submit" >立即查询</button>
        </form>
    </view>
